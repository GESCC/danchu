<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Food">
	<select id="getAllFoodList" resultType="com.danchu.momuck.vo.Food">
		select *
		from food
	</select>
	
	<select id="getCategoryFoodList" resultType="com.danchu.momuck.vo.Food">
		select *
		from food
		where category = #{category}
	</select>
</mapper>